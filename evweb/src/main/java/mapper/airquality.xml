<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.airquality">
	<insert id="insert" parameterType="airquality">
		insert into airquality_tb values(
		#{stationname}, #{datatime}, #{pm10value}, #{pm25value})
	</insert>
	<delete id="delete">
		delete from airquality_tb 
	</delete>
	<select id="select" parameterType="String" resultType="airquality">
		select * from airquality_tb where stationname = #{stationname} order by weatherrow asc
	</select>
	
</mapper>